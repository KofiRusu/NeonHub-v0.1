# Database Configuration
DATABASE_URL="postgresql://neonhub:secret@neonhub-db:5432/neonhub?schema=public"

# Server Configuration
PORT=3001
NODE_ENV=production

# Authentication
JWT_SECRET=your_jwt_secret_here

# API Keys
OPENAI_API_KEY=your_openai_api_key_here

# Logging
